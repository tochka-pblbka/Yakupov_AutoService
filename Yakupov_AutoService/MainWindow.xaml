<Window x:Class="Yakupov_AutoService.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Yakupov_AutoService"
        mc:Ignorable="d"
        Title="Yakupova_Autoservice" Height="450" Width="800" FontFamily="Tw Cen MT" Icon="/res/service_logo.ico">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="75"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="30"/>
        </Grid.RowDefinitions>
        <Image Source="/res/service_logo.png" HorizontalAlignment="Left"></Image>
        <TextBlock Text="Услуги автосервиса" HorizontalAlignment="Center" VerticalAlignment="Center" FontSize="30"></TextBlock>
        <Frame Grid.Row="1" Name="MainFrame" ContentRendered="MainFrame_ContentRendered" NavigationUIVisibility="Hidden"></Frame>
        <Button Content="Назад" Name="BtnBack" HorizontalAlignment="Right" Click="BtnBack_Click"></Button>
    </Grid>
</Window>
